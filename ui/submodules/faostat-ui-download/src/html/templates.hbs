<template id="main_structure">
    <div id="infoForm_modal" style="display: none;width:400px; ">
        <div class="modal" data-backdrop="static" data-keyboard="false" id="infoForm_modal_contentBody" style="background-color: white; opacity: 1.0; top: 15%;">

            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <br><br>
                    <label for="name"> Please fill in the following form before downloading the data</label>

                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <label for="name">Full Name</label>
                    <input type="name" class="form-control" id="name" placeholder="Enter full name" name="name">
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <label for="email">Email Address</label>
                    <input type="email" class="form-control" id="email" placeholder="Enter email address" name="email">
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <!--input type='radio' value='Table' name='{{prefix}}output_type' id='{{prefix}}output_type' {{output_type_table_checked}}-->
                    <label for="{{prefix}}output_type">{{table_label}}</label>
                    <label for="institution">Type Institution</label>
                    <div>
                    <select id="typeInstitution" style="height: 35px; width: 100%;  border-radius: 8px 8px 8px 8px; font-family: Open Sans,sans-serif;" name="institutionType" >

                        <option value="Ministry">Ministry</option>

                        <option value="National Statistical Office">National Statistical Office</option>

                        <option value="Academia/Research Institute">Academia/Research Institute</option>

                        <option value="Private Sector">Private Sector</option>

                        <option value="Media">Media</option>

                        <option value="NGO/CSO">NGO/CSO</option>

                        <option value="FAO">FAO</option>

                        <option value="Other UN Agencies">Other UN Agencies</option>

                        <option value="Other International Organizations/Financial Institutions">Other International Organizations/Financial Institutions</option>

                        <option value="Other">Other</option>
                    </select>
                    </div>
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-xs-8 col-xs-offset-2">
                    <label for="institution">Institution</label>
                    <input type="institution" class="form-control" id="institution" placeholder="Enter Institution" name="institution">

                </div>
            </div>
            <div class="row">
                <div class="col-xs-3 col-xs-offset-2">
                    <div id="captcha_element"></div>
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-xs-3 col-xs-offset-5">
                    <button type="button" class="btn btn-default" id="infoFormSubmit">Submit</button>
                </div>
            </div>
            <div class="row">
                <div id ='infoFormError' class="col-xs-11 col-xs-offset-2" style="display: none; color: red">
                    <h5>userInfoFormErrorMsg</h5>
                </div>
            </div>

        </div>
    </div>
    <div class="panel panel-default hoverable">
        <div class="panel-body">

            <div style="min-height:300px;" data-role="selectors"></div>

            <div class="fs-download-options">

                <div class="panel-body">
                    <div class="row">

                        <div class="col-md-12">
                            <div data-role="options"></div>
                        </div>

                    </div>

                    <div class="row fs-download-options-buttons">

                        <div class="hidden-xs col-sm-4"></div>

                        <div class="col-xs-6 col-sm-4">
                            <button class="btn btn-primary btn-block waves-effect truncate" data-role="preview">
                                <i class="material-icons left">search</i>
                                {{show_data}}
                            </button>
                        </div>

                        <div class="col-xs-6 col-sm-4">
                            <button class="btn btn-default btn-block waves-effect waves-light truncate" data-role="export">
                                <i class="material-icons left">file_download</i>
                                {{download_data}}
                            </button>
                            <!--<p class="pull-right"><small>Download in CSV with English formatting</small></p>-->
                        </div>

                    </div>

                </div>

            </div>
        </div>
    </div>
    <!--<div data-role="output-area"></div>-->
</template>

<template id="onboarding">
    <a class="btn btn-info btn-block waves-effect truncate" data-role="onboarding">
        <i class="material-icons left">help_outline</i>
        <span data-role="text">{{onboarding_text}}</span>
    </a>
</template>


